{
    "manifest_version": 2,
    "name": "Monvert",
    "version": "1.0",
    "description": "Converts selected dollar amounts to rupees.",
    "permissions": ["activeTab"],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["contentScript.js"]
      }
    ],
    "icons": {
      "48": "icon.png"
    }
  }
  